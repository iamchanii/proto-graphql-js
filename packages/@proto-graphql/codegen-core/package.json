{
  "name": "@proto-graphql/codegen-core",
  "version": "0.4.0",
  "description": "",
  "keywords": [
    "graphql",
    "grpc",
    "nexus",
    "protobuf",
    "typescript"
  ],
  "repository": "git@github.com:proto-graphql/proto-graphql-js.git",
  "author": "izumin5210 <m@izum.in>",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "private": false,
  "engines": {
    "node": ">= 14.0.0"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^1.8.0",
    "@bufbuild/protoplugin": "^1.8.0",
    "case-anything": "^2.1.13",
    "change-case": "^4.1.2",
    "ts-poet": "^6.3.0"
  },
  "devDependencies": {
    "@proto-graphql/tsconfig": "workspace:*"
  },
  "files": [
    "src/",
    "dist/",
    "!src/**/*.test.ts",
    "!src/**/__tests__/"
  ],
  "scripts": {
    "build": "tsup",
    "prebuild": "../../../scripts/compile-extensions-proto",
    "test": "vitest run --passWithNoTests",
    "test:coverage": "vitest run --coverage --passWithNoTests"
  }
}
