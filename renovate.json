{
  "extends": ["config:base"],
  "packageRules": [
    {
      "matchPaths": ["**"],
      "enabled": false
    },
    {
      "matchPaths": ["package.json"],
      "enabled": true,
      "rangeStrategy": "auto"
    },
    {
      "matchDepTypes": ["dependencies", "peerDependencies"],
      "labels": ["dependencies"]
    },
    {
      "matchDepTypes": ["devDependencies"],
      "labels": ["devDependencies"],
      "rangeStrategy": "pin"
    }
  ]
}
